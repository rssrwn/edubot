<div class="classBox">
  <h2 class="className">{{class_name}}</h2>

  <table class="classMembersTable">
    <tr>
      <th>Username</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Age</th>
      <td></td>
    </tr>
    {{#each class_members}}
      <tr class="studentRow">
        <td>{{this.uname}}</td>
        <td>{{this.fname}}</td>
        <td>{{this.lname}}</td>
        <td>{{this.age}}</td>
        <td><a class="removeStudentButton" onclick="removeMember({{class_name}}, {{this.uname}})">x</a></td>
      </tr>
    {{/each}}
  </table>

  <button type="button" onclick="addMemberEvent({{{class_id}}}, this)">Add Member</button>
</div>
