<div class="classBox">
  <h2 class="className">{{class_name}}</h2>

  <table class="classMembersTable">
    <tr>
      <th>Username</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Age</th>
      <td></td>
    </tr>
    {{#each class_members}}
      <tr class="studentRow">
        <td class="studentLinkButton">{{this.uname}}</td>
        <td class="studentLinkButton">{{this.fname}}</td>
        <td class="studentLinkButton">{{this.lname}}</td>
        <td class="studentLinkButton">{{this.age}}</td>
        <td><img src="../images/remove_member.png" onmouseover="this.src='../images/remove_member_hover.png'" onmouseout="this.src='../images/remove_member.png'" class="removeStudentButton" onclick="removeMember({{../class_id}}, '{{../class_name}}', '{{this.uname}}')"/></td>
      </tr>
    {{/each}}
  </table>

  <button class="classButton" type="button" onclick="removeClass({{class_id}}, '{{class_name}}')">Remove Class</button>
  <button class="classButton" type="button" onclick="addMemberEvent({{{class_id}}}, this)">Add Member</button>
</div>
